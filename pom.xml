        <version.org.fenixedu.fenixedu-commons>2.0.0</version.org.fenixedu.fenixedu-commons>
        <version.org.fenixedu.academic>18.5.0</version.org.fenixedu.academic>
        <version.org.fenixedu.cms>3.1.2</version.org.fenixedu.cms>
        <version.org.fenixedu.fenixedu-learning>3.1.5</version.org.fenixedu.fenixedu-learning>
        <version.com.google.code.gson.gson>2.8.0</version.com.google.code.gson.gson>
        <version.com.google.guava.guava>21.0</version.com.google.guava.guava>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.fenixedu</groupId>
            <artifactId>fenixedu-academic</artifactId>
            <version>${version.org.fenixedu.academic}</version>
        </dependency>
        <dependency>
            <groupId>org.fenixedu</groupId>
            <artifactId>fenixedu-cms</artifactId>
